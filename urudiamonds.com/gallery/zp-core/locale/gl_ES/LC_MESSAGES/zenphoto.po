msgstr "Protección de imaxe completa: "

#: zenphoto/zp-core/admin-options.php:1600
msgid "Unprotected"
msgstr "Desprotexida"

#: zenphoto/zp-core/admin-options.php:1600
msgid "Protected view"
msgstr "Vista protexida"

#: zenphoto/zp-core/admin-options.php:1600
msgid "No access"
msgstr "Ningún acceso"

#: zenphoto/zp-core/admin-options.php:1607
msgid "Disable hotlinking"
msgstr "Hotlinking deshabitado"

#: zenphoto/zp-core/admin-options.php:1612
msgid "cache the full image"
msgstr "caché das imaxes completas"

#: zenphoto/zp-core/admin-options.php:1621
#: zenphoto/zp-core/admin-options.php:1669
msgid "password:"
msgstr "contrasinal:"

#: zenphoto/zp-core/admin-options.php:1662
